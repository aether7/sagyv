body
    font-size 16px

#login
    background #fafafa
    margin auto
    padding 3em
    width 50%

    #mensaje
        padding 0.5em

    div:last-child
        padding 1em 0

#main_content
    clear both

#modal_ver
    .form-group
        margin 0
        padding 0 0.5em

        .control-label,
        .form-control-static
            border 1px solid #e6e6e6
            display inline-block
            height 40px
            padding 0.5em
            vertical-align top

        .control-label
            background #f0f0f0
            width 40%

        .form-control-static
            width 59%

#liquidacion
    .table
        .fila-desglose
            td
                text-align right


#my_tab
    margin 2em 0

.auto-margin
    margin auto

.box
    box-sizing border-box
    display inline-block
    vertical-align top

.block
    display block

.center
    text-align center

.logo-index
    float left

for num in (5..95)
    s(".w%s{width:%s%}", num, num)
